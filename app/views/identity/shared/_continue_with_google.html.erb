<div>
  <div id="g_id_onload"
       data-client_id="<%= Rails.application.credentials.google_oauth.client_id %>"
       data-login_uri="<%= google_accounts_url %>"
       data-context="signin"
       data-ux_mode="redirect"
       data-auto_prompt="false"
       data-authenticity_token="<%= form_authenticity_token %>">
  </div>

  <div data-type="standard"
       data-shape="rectangular"
       data-theme="outline"
       data-text="continue_with"
       data-size="large"
       data-logo_alignment="left"
       data-authenticity_token="<%= form_authenticity_token %>"
       data-width="250">
  </div>
</div>
